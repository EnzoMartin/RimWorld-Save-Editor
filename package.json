{
  "name": "rimworld-save-editor",
  "version": "2.0.0",
  "description": "RimWorld save editor",
  "repository": {
    "type": "git",
    "url": "git://github.com/EnzoMartin/RimWorld-Save-Editor.git"
  },
  "license": "MIT",
  "author": "Enzo Martin <enzo.r.martin@gmail.com>",
