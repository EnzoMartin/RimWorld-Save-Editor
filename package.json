  "dependencies": {
    "bunyan": "^1.8.1",
    "rimworld-definitions-nodejs": "^0.15.1284",
    "semver": "^5.3.0",
    "singleton-class": "^1.0.4"
  },
  "devDependencies": {
    "enzo-codestyle": "^4.3.0",
    "eslint": "^4.10.0",
    "expect": "^21.2.1",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "proxyquire": "^1.7.10"
